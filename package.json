{
  "name": "zattire-admin",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc -p tsconfig.json && npm run copy:emails",
    "start:dev": "nodemon --watch src -x ts-node src/server.ts",
    "start:dev:test": "set NODE_ENV=test && nodemon -x ts-node src/server.ts",
    "test": "set NODE_ENV=test && jest --no-cache --runInBand ",
    "testU": "set NODE_ENV=test && jest /src/__tests__/UserTest/salonInfo.test.ts --no-cache --runInBand --watch --detectOpenHandles",
    "vcheck": "tsc -v",
    "seed:dev": "seed -u 'mongodb://127.0.0.1:27017/zattire' ./src/seeds/data",
    "tscc": "tsc -v",
    "copy:emails": "cp -r src/utils/emails/*.html dist/utils/emails"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": false,
    "moduleFileExtensions": [
      "js",
      "tsx",
      "ts"
    ],
    "collectCoverageFrom": [
      "**/service/*.{ts,js,jsx}",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "collectCoverage": false,
    "coverageDirectory": "coverage",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.788.0",
    "axios": "^0.21.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-rate-limit": "^5.1.3",
    "express-validator": "^6.6.0",
    "firebase-admin": "^9.3.0",
    "html-pdf": "^2.2.0",
    "jwt-then": "^1.0.1",
    "moment": "^2.29.1",
    "mongo-seeding-cli": "^3.5.0",
    "mongoose": "^5.10.13",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.4.15",
    "normalize-date": "0.0.4",
    "pg": "^8.4.2",
    "pg-hstore": "^2.3.3",
    "rate-limit-redis": "^2.0.0",
    "razorpay": "^2.0.6",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^3.0.1",
    "swagger-jsdoc": "^6.0.0",
    "swagger-ui-express": "^4.1.6",
    "typescript": "^4.0.5",
    "winston": "^3.2.1",
    "winston-cloudwatch": "^2.3.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/faker": "^5.1.4",
    "@types/html-pdf": "^2.2.0",
    "@types/jest": "^26.0.15",
    "@types/jwt-then": "^1.0.0",
    "@types/mongoose": "^5.7.37",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.4",
    "@types/multer-s3": "^2.7.8",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.28",
    "@types/socket.io": "^2.1.11",
    "@types/supertest": "^2.0.8",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/validator": "^13.1.0",
    "faker": "^5.1.0",
    "jest": "^26.6.3",
    "mockgoose": "^8.0.4",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "validator": "^13.1.17"
  }
}
