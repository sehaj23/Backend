{
  "name": "zattire-admin",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc -p tsconfig.json",
    "start:dev": "nodemon -x ts-node src/server.ts",
    "start:t": "set NODE_ENV=test && nodemon -x ts-node src/server.ts",
    "test": "set NODE_ENV=test && jest --no-cache --runInBand ",
    "testU": "set NODE_ENV=test && jest /src/__tests__/UserTest/salonInfo.test.ts --no-cache --runInBand --watch --detectOpenHandles",
    "vcheck": "tsc -v",
    "seed:dev": "seed -u 'mongodb://127.0.0.1:27017/zattire' ./src/seeds/data"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": false,
    "moduleFileExtensions": [
      "js",
      "tsx",
      "ts"
    ],
    "collectCoverageFrom": [
      "**/service/*.{ts,js,jsx}",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "collectCoverage": false,
    "coverageDirectory": "coverage",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.651.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "express-validator": "^6.6.0",
    "firebase-admin": "^8.12.1",
    "jwt-then": "^1.0.1",
    "moment": "^2.27.0",
    "mongo-seeding-cli": "^3.4.1",
    "mongoose": "^5.9.7",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "normalize-date": "0.0.4",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "rate-limit-redis": "^2.0.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^2.3.0",
    "typescript": "^3.8.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.11",
    "@types/jest": "^25.2.1",
    "@types/jwt-then": "^1.0.0",
    "@types/mongoose": "^5.7.8",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.2",
    "@types/multer-s3": "^2.7.7",
    "@types/redis": "^2.8.22",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^12.0.1",
    "faker": "^4.1.0",
    "jest": "^25.5.4",
    "mockgoose": "^8.0.4",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.10.2",
    "validator": "^13.1.1"
  }
}
