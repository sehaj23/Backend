{ 
    "compilerOptions":{ 
       "target":"es6",
       "module":"commonjs",
       "declaration": true,
       "emitDecoratorMetadata":true,
       "experimentalDecorators":true,
       "outDir":"dist/",
       "listFiles":true,
       "noEmitHelpers":false,
       "alwaysStrict": true,
       "suppressImplicitAnyIndexErrors": false,
       "noImplicitAny": false
    },
    "include":[ 
       "src/**/*"
    ],
    "exclude":[ 
       "node_modules"
    ],
    "compileOnSave":false
}